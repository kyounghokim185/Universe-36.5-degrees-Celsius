{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/backend/main.py"
        }
    ],
    "builds": [
        {
            "src": "src/main.jsx",
            "use": "@vercel/vite"
        },
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        }
    ]
}